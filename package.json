{
  "name": "gulp-frontend",
  "version": "1.0.0",
  "description": "Gulp frontend start",
  "scripts": {
    "dev": "gulp watch",
    "build:dev": "gulp build",
    "build:prod": "gulp build --prod"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && npm run foo"
    }
  },
  "keywords": [
    "gulp",
    "js",
    "sass",
    "pug",
    "linters"
  ],
  "author": "Julian",
  "license": "WTFPL",
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/eslint-parser": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/register": "^7.13.8",
    "airbnb": "0.0.2",
    "base": "^3.0.0",
    "eslint": "^7.22.0",
    "eslint-config": "^0.3.0",
    "eslint-plugin-import": "^2.22.1",
    "gulp": "^4.0.2",
    "husky": "^6.0.0",
    "i": "^0.3.6",
    "lint-staged": "^10.5.4",
    "npm": "^7.6.3"
  },
  "dependencies": {
    "eslint-config-airbnb-base": "^14.2.1"
  }
}
